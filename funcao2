#include <stdio.h>

int eh_primo (int n){
    int i;

    for(i=2; i<n; i++){
        if (n%i==0){
            return 0;
        }
        else{
            return 1;
        }
    }

}

int todos_os_primos (int a){
    int i=2;
    
    while(i<=a){
       if (eh_primo(i)){
        printf("%d\n", i);
       };
        
       i++;
    }
return 0;
}

void main (){
    int max;
    printf("Insira um nÃºmero: ");
    scanf("%d", &max);
    todos_os_primos(max);
}
